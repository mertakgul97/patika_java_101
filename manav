package manavKasa;

import java.util.Scanner;

public class manavKasa {

	public static void main(String[] args) {
		Scanner scan = new Scanner(System.in);
		double armut = 2.14, elma = 3.67, domates = 1.11, muz = 0.95, patlican = 5.00, total;
		System.out.print("Armut kaç kilo: ");
		double perArmut = scan.nextDouble();
		System.out.print("Elma kaç kilo: ");
		double perElma = scan.nextDouble();
		System.out.print("Domates kaç kilo: ");
		double perDomates = scan.nextDouble();
		System.out.print("Muz kaç kilo: ");
		double perMuz = scan.nextDouble();
		System.out.print("Patlican kaç kilo: ");
		double perPatlican = scan.nextDouble();
		total = (armut * perArmut) + (perElma * elma) + (domates * perDomates) + (muz * perMuz)
				+ (patlican * perPatlican);
		System.out.println("Toplam tutar: " + total + " TL");
	}

}
